{
  "name": "@futpib/parser",
  "version": "1.0.7",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "license": "GPL-3.0-only",
  "type": "module",
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "tsd": "tsd --files 'src/**/*.test-d.ts'",
    "test": "c8 ava",
    "prepack": "yarn build"
  },
  "homepage": "https://github.com/futpib/parser",
  "ava": {
    "typescript": {
      "rewritePaths": {
        "src/": "build/"
      },
      "compile": false
    },
    "verbose": true,
    "timeout": "60s"
  },
  "tsd": {
    "directory": "src",
    "compilerOptions": {
      "lib": [
        "ES2022",
        "DOM",
        "DOM.AsyncIterable"
      ]
    }
  },
  "devDependencies": {
    "@ava/typescript": "^6.0.0",
    "@fast-check/ava": "^2.0.2",
    "@futpib/fetch-cid": "^1.0.2",
    "@gruhn/regex-utils": "^2.9.1",
    "@types/estree": "^1.0.8",
    "@types/invariant": "^2.2.37",
    "@types/node": "^24.10.1",
    "ava": "^6.4.1",
    "bson": "^7.0.0",
    "c8": "^10.1.3",
    "coveralls": "^3.1.1",
    "env-paths": "^3.0.0",
    "eslint-config-xo-typescript-overrides": "^2.0.3",
    "execa": "^9.6.0",
    "fast-check": "^4.3.0",
    "invariant": "^2.2.4",
    "jszip": "^3.10.1",
    "leb128": "^0.0.5",
    "mutf-8": "^1.2.2",
    "p-memoize": "^8.0.0",
    "s-expression": "^3.1.1",
    "tempy": "^3.1.0",
    "tsd": "^0.33.0",
    "type-fest": "^5.2.0",
    "typescript": "^5.9.3",
    "xo": "^1.2.3"
  },
  "packageManager": "yarn@4.9.4",
  "dependencies": {
    "acorn": "^8.15.0",
    "astring": "^1.9.0",
    "fp-ts": "^2.16.11",
    "mem": "^10.0.0",
    "monocle-ts": "^2.3.13",
    "newtype-ts": "^0.3.5",
    "p-mutex": "^1.0.0"
  },
  "peerDependencies": {
    "mutf-8": "^1.1.4"
  },
  "peerDependenciesMeta": {
    "mutf-8": {
      "optional": true
    }
  }
}
